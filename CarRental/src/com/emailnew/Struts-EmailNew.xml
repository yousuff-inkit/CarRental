<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"  
    "http://struts.apache.org/dtds/struts-2.0.dtd">
 
<struts>
    <constant name="struts.multipart.maxSize" value="10485760" /> <!-- 10 MB -->
 
    <package name="Struts2EmailNew" extends="json-default">      

        <action name="SendEmailNew" class="com.emailnew.SendEmailAction" method="doSendEmail">
 			<interceptor-ref name="params"/>
				<result name="success" >Email.jsp</result>
				<result name="fail">Email.jsp</result>        
        </action>
    </package>
</struts>
