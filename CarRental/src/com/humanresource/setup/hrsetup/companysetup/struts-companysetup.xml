<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	 
	<package name="companysetup" extends="struts-default" namespace="/">
			<action name="saveCompanysetup" class="com.humanresource.setup.hrsetup.companysetup.ClsCompanysetupAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success" >companysetup.jsp</result>
				<result name="fail">companysetup.jsp</result>
			</action>
		</package>	
</struts>