<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	 
	<package name="bankdetails" extends="struts-default" namespace="/">
			<action name="saveBankdetails" class="com.humanresource.setup.hrsetup.bankdetails.ClsBankdetailsAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success" >bankdetails.jsp</result>
				<result name="fail">bankdetails.jsp</result>
			</action>
		</package>	
</struts>