<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="taxclose" extends="struts-default">
			<action name="saveTaxClose" class="com.controlcentre.settings.taxclose.ClsTaxCloseAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success" >taxClose.jsp</result>
				<result name="fail">taxClose.jsp</result>
			</action>
					</package>	
</struts>