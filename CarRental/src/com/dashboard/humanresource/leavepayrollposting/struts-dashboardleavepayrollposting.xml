<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="leavepayrollposting" extends="struts-default">
			<action name="saveDashboardLeavePayrollPosting" class="com.dashboard.humanresource.leavepayrollposting.ClsLeavePayrollPostingAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success">leavepayrollposting.jsp</result>
				<result name="fail">leavepayrollposting.jsp</result>
				
			</action>

		</package>	
</struts>
