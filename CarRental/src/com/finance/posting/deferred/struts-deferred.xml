<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	

	<package name="deferred" extends="struts-default">
			<action name="savedeferred" class="com.finance.posting.deferred.ClsDeferredAction" method="saveAction">
				<interceptor-ref name="params"/>
				<result name="success">deferred.jsp</result>
				<result name="fail">deferred.jsp</result>
			</action>
			
			<action name="printdeferred" class="com.finance.posting.deferred.ClsDeferredAction" method="printAction">
				<interceptor-ref name="params"/>
				<result name="print">deferred.jsp</result>
			</action>
			
		</package>	
</struts>

