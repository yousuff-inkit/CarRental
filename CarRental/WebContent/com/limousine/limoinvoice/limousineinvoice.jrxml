		<queryString>
			<![CDATA[select IF(sum(a.vatval) = 0, 'Exempt', CONCAT('Standard Rate ', '(', IF (sum(a.vatval) != 0,'5',ROUND(a.VATPER, 0)), '%)')) taxdesc,ROUND(@i:=@i+1 ,0) srno,concat(a.code,' ',sum(a.rate)) taxrate,concat(a.code,' ',sum(a.vatval)) taxvatval,concat(a.code,' ',sum(a.AMOUNT)) taxamount,a.* from(
		select  c.code,ROUND(invd.tarif,2) RATE,ROUND(INVD.VATPERCENT,2) VATPER,ROUND(INVD.tarif/100*ROUND(INVD.VATPERCENT,2),2) VATVAL,ROUND(coalesce(INVD.TARIF,0)+COALESCE(INVD.tarif/100*ROUND(INVD.VATPERCENT,2),0),2) AMOUNT From gl_limoinvm inv left join gl_limoinvd invd on invd.rdocno=inv.doc_no left join my_curr c on c.doc_no=inv.curid  where inv.DOC_NO=$P{docno} 
		UNION ALL select c.code,SUM(ROUND(invd.exhrchg,2)) RATE,ROUND(INVD.VATPERCENT,2) VATPER, SUM(ROUND(INVD.exhrchg/100*ROUND(INVD.VATPERCENT,2),2)) VATVAL, SUM(ROUND(coalesce(INVD.exhrchg,0)+COALESCE(INVD.exhrchg/100*ROUND(INVD.VATPERCENT,2),0),2)) AMOUNT From gl_limoinvm inv left join gl_limoinvd invd on invd.rdocno=inv.doc_no  left join my_curr c on c.doc_no=inv.curid where inv.DOC_NO=$P{docno}  AND INVD.exhrchg!=0 
		UNION ALL select   c.code,SUM(ROUND(invd.parkingchg,2)) RATE,0 VATPER, 0 VATVAL, SUM(ROUND(coalesce(INVD.parkingchg,0),2)) AMOUNT From gl_limoinvm inv left join gl_limoinvd invd on invd.rdocno=inv.doc_no left join my_curr c on c.doc_no=inv.curid where inv.DOC_NO=$P{docno} AND INVD.parkingchg!=0
		UNION ALL select   c.code,SUM(ROUND(invd.fuelchg,2)) RATE,0 VATPER, 0 VATVAL, SUM(ROUND(coalesce(INVD.fuelchg,0),2)) AMOUNT From gl_limoinvm inv left join gl_limoinvd invd on invd.rdocno=inv.doc_no left join my_curr c on c.doc_no=inv.curid where inv.DOC_NO=$P{docno} AND INVD.fuelchg!=0
		UNION ALL select   c.code,SUM(ROUND(invd.otherchg,2)) RATE,0 VATPER, 0 VATVAL, SUM(ROUND(coalesce(INVD.otherchg,0),2)) AMOUNT From gl_limoinvm inv left join gl_limoinvd invd on invd.rdocno=inv.doc_no left join my_curr c on c.doc_no=inv.curid where inv.DOC_NO=$P{docno} AND INVD.otherchg!=0
		)a,(select @i:=0)b]]>
		</queryString>
