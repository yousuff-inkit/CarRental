			     coalesce(ac.mail1)) clientdetails,comp.company,comp.address ,comp.tel,comp.fax,br.branchname,inv.voc_no vocno,inv.cldocno ,
			     date_format(inv.date,'%d-%m-%Y') date,date_format(inv.fromdate,'%d-%m-%Y') fromdate,date_format(inv.todate,'%d-%m-%Y') todate,
			     ac.refname from gl_limoinvm inv left join (select sum(total) mastertotal,sum(vatvalue) mastervat, sum(total) +sum(vatvalue) masternettotal,rdocno from gl_limoinvd invd  group by  invd.rdocno ) invd on inv.doc_no=invd.rdocno left join my_acbook ac on (inv.cldocno=ac.cldocno and ac.dtype='CRM') left join my_brch br on 
